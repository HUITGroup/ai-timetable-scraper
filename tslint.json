{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": true,
    "interface-name": false,
    "import-name": false,
    "jsx-no-lambda": false,
    "no-console": false,
    "no-empty-interface": false,
    "object-literal-sort-keys": false,
    "ordered-imports": true,
    "semicolon": true,
    "trailing-comma": true
  }
}
